<Legislation RestrictExtent="E+W+S+N.I.">
<Primary>
<Body>
  <Part id="part-I" RestrictExtent="E+W+S+N.I.">
    <Number>Part I</Number>
    <Title>General Provisions</Title>
    <Pblock id="xh-preliminary">
      <Title>Preliminary</Title>
      <P1group>
        <P1 id="section-1">
          <Pnumber>1</Pnumber>
          <P1para>
            <P2 id="section-1-1">
              <Pnumber>1</Pnumber>
              <P2para>
                <Text>The provisions of this Part shall have effect with a view toâ€”</Text>
                <P3 id="section-1-1-a">
                  <Pnumber>a</Pnumber>
                  <P3para><Text>securing the health and safety of persons at work;</Text></P3para>
                </P3>
                <P3 id="section-1-1-b">
                  <Pnumber>b</Pnumber>
                  <P3para><Text>protecting persons other than persons at work.</Text></P3para>
                </P3>
              </P2para>
            </P2>
            <P2 id="section-1-2">
              <Pnumber>2</Pnumber>
              <P2para>
                <Text>The provisions relating to the making of regulations shall have effect.</Text>
              </P2para>
            </P2>
          </P1para>
        </P1>
      </P1group>
    </Pblock>
    <Pblock id="xh-duties">
      <Title>General duties</Title>
      <P1group>
        <P1 id="section-2">
          <Pnumber>2</Pnumber>
          <P1para>
            <P2 id="section-2-1">
              <Pnumber>1</Pnumber>
              <P2para>
                <Text>It shall be the duty of every employer to ensure the health and safety.</Text>
              </P2para>
            </P2>
          </P1para>
        </P1>
      </P1group>
    </Pblock>
  </Part>
  <Part id="part-II">
    <Number>Part II</Number>
    <Title>Enforcement</Title>
    <Pblock id="xh-enforcement">
      <Title>Enforcement provisions</Title>
      <P1group>
        <P1 id="section-3">
          <Pnumber>3</Pnumber>
          <P1para>
            <Text>The enforcing authority shall enforce these provisions.</Text>
          </P1para>
        </P1>
      </P1group>
    </Pblock>
  </Part>
</Body>
</Primary>
</Legislation>
